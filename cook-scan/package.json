{
  "name": "cook-scan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- next dev --turbopack",
    "build": "dotenv -e .env -- next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "db:generate": "prisma generate",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "db:migrate:dev": "dotenv -e .env.migration -- prisma migrate dev --name ${npm_config_name}",
    "db:push:dev": "dotenv -e .env.migration -- prisma db push",
    "db:seed:dev": "dotenv -e .env.migration -- prisma db seed",
    "db:reset:dev": "dotenv -e .env.migration -- prisma migrate reset",
    "db:studio:dev": "dotenv -e .env.migration -- prisma studio",
    "db:migrate:status": "dotenv -e .env -- prisma migrate status",
    "db:migrate:deploy": "dotenv -e .env -- prisma migrate deploy",
    "db:seed:prod": "dotenv -e .env -- prisma db seed",
    "db:studio:prod": "dotenv -e .env -- prisma studio"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.29",
    "@ai-sdk/openai": "^2.0.64",
    "@mastra/core": "^0.24.0",
    "@mastra/libsql": "^0.16.2",
    "@mastra/loggers": "^0.10.19",
    "@mastra/memory": "^0.15.11",
    "@prisma/client": "^6.12.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "ai": "^5.0.0",
    "next": "15.4.3",
    "prisma": "^6.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.0",
    "dotenv-cli": "^9.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "jsdom": "^27.1.0",
    "mastra": "^0.18.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8",
    "vitest-mock-extended": "^3.1.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }

{
  "name": "cook-scan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- next dev --turbopack",
    "build": "dotenv -e .env -- next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "db:migrate:dev": "dotenv -e .env.migration -- prisma migrate dev --name ${npm_config_name}",
    "db:push:dev": "dotenv -e .env.migration -- prisma db push",
    "db:seed:dev": "dotenv -e .env.migration -- prisma db seed",
    "db:reset:dev": "dotenv -e .env.migration -- prisma migrate reset",
    "db:studio:dev": "dotenv -e .env.migration -- prisma studio",
    "db:migrate:status": "dotenv -e .env -- prisma migrate status",
    "db:migrate:deploy": "dotenv -e .env -- prisma migrate deploy",
    "db:seed:prod": "dotenv -e .env -- prisma db seed",
    "db:studio:prod": "dotenv -e .env -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "next": "15.4.3",
    "prisma": "^6.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "uuid": "^11.1.0",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^9.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
